// Encontrar el numero menor dentro de una cadena

let arregloNumero = [8,7,9,4,6,2,1] // aqui definimos el arreglo

// menor = 8
// 8 <= 7 F
// menor = 7
//7 <= 9 V
// menor = 7
// 7 <= 4 F
// menor= 4
//4 <= 6 V
//menor = 4
// 4 <= 2 F
// menor = 2
//2 <= 1 F
// menor = 1

let numMenor = arregloNumero [0]

	for(let i = 0; i < arregloNumero.length; i++) {

     		if (!(numMenor <=arregloNumero [i])){

			numMenor = arregloNumero [i]
}	

console.log("El número menor de la iteración", i , numMenor)

}

console.log( " El número menor es", numMenor)

