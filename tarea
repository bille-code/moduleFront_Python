//Ordenar de un arreglo de menor a mayor

let unorderedNumbers = [3,8, 7, 4, 459, 3, 21]

function sort(array) {
   for (let i = 0; i < array.length; i++) {
       for (let j = i + 1; j < array.length; j++ ) { 
	     let actual = array[i]
             let siguiente = array[j] 
             if (actual > siguiente) {
        // intercambiamos
        array[i] = siguiente
        array[j] = actual
      }      if (actual <= siguiente) {
        // no hacemos nada, continuamos
             continue
      }
    }
  }
  return array
}console.log(sort(unorderedNumbers))

