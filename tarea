//Tablas de Multiplicar del 1 al 10

var resultado

 for(let multi = 1; multi <= 10; multi ++ ){
    console.log("Tabla del ", multi)
    for (let i = 1; i <= 10; i++ ){
      resultado = multi * i
      console.log( multi, 'x', i, '=', resultado )
    }
}
17:53
multi = 1 iteracion1
  multi * (i = 1) = 1
  multi * (i = 2) = 2
  multi * (i = 3) = 3
  .
  .
  multi * (i = 10) = 10
multi = 2 iteracion2
  multi * (i = 1) = 2
  multi * (i = 2) = 4
  .
  .
  .
multi = 3
  multi * (i = 1) = resultado

